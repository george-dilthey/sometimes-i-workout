<div class = search>
  <%= form_tag user_workouts_path, method: :get do %>
      <div class="form-group">
        <%= label_tag :search %>
        <%= text_field_tag :search, @params[:search], placeholder: "Search for a Workout",  class: "form-control " %><br><br> 

      </div>
      <div class = "text-center">
        <%= submit_tag "Search", class: "btn btn-light"%>
      </div><br><br>
  <%end%>
</div>

